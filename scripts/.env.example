# Configuration pour l'API Confluence
# Copiez ce fichier vers scripts/.env et remplissez avec vos valeurs

# URL de base de votre instance Confluence
export CONFLUENCE_URL="https://wiki.uqam.ca"

# Token d'accès personnel (Personal Access Token)
# Obtenez-le sur : https://wiki.uqam.ca/plugins/personalaccesstokens/usertokens.action
export CONFLUENCE_TOKEN="votre_token_ici"

# Clé de l'espace Confluence
export CONFLUENCE_SPACE="UQAMGPT"

# Section parente (ID de la page parente, optionnel)
export PARENT_PAGE_ID="255302222"
